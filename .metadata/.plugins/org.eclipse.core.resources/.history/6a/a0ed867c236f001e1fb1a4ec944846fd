package firstJavaProject;
import java.util.Scanner;

public class Activity4 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		System.out.println("숫자를 입력하세요");
		int num = scan.nextInt();
		
		for( int i = 1; i <= num; i++ ) {
			System.out.print(i);
			i++;
		}
		
		scan.close();

	}

}
